{
  "name": "api_server",
  "version": "1.4.32",
  "description": "API Server of PCMS",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10.16"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "debug": "nodemon ./dist/index.js && npm run build:watch",
    "lint": "npm run eslint && npm run prettier:check",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --ext .ts --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "test": "lb-mocha --allow-console-logs --recursive \"dist/__tests__\" --reporter mochawesome --reporter-options reportDir=test-report,reportFilename=api-unit-tests.html,json=false",
    "test:report": ".\\generateTestReport.bat",
    "migrate": "node ./dist/migrate",
    "migrate:rebuild": "node ./dist/migrate -- --rebuild",
    "openapi-spec": "node ./dist/openapi-spec",
    "start": "node -r source-map-support/register .",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "12": "^1.0.2",
    "@loopback/authentication": "^7.0.5",
    "@loopback/authentication-jwt": "^0.7.5",
    "@loopback/authorization": "^0.7.5",
    "@loopback/boot": "^3.1.2",
    "@loopback/core": "^2.13.1",
    "@loopback/openapi-v3": "^5.1.2",
    "@loopback/repository": "^3.3.0",
    "@loopback/rest": "^9.1.1",
    "@loopback/rest-explorer": "^3.0.5",
    "@loopback/security": "^0.3.5",
    "@loopback/service-proxy": "^3.0.5",
    "@types/jest": "^26.0.14",
    "bcryptjs": "^2.4.3",
    "casbin": "^5.1.6",
    "casbin-sequelize-adapter": "^2.1.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^8.2.0",
    "fs": "0.0.1-security",
    "lodash": "^4.17.20",
    "loopback-connector-mssql": "^3.8.0",
    "multer": "^1.4.2",
    "speakeasy": "^2.0.0",
    "tls": "0.0.1",
    "tslib": "^2.0.0"
  },
  "prettier": "eslint-config-prettier",
  "devDependencies": {
    "@loopback/build": "^6.2.8",
    "@loopback/eslint-config": "^10.0.4",
    "@loopback/testlab": "^3.2.10",
    "@types/bcryptjs": "^2.4.2",
    "@types/express-serve-static-core": "^4.17.20",
    "@types/lodash": "^4.14.161",
    "@types/mocha": "^8.0.4",
    "@types/multer": "^1.4.5",
    "@types/node": "^10.17.48",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "mocha": "^8.2.1",
    "mochawesome": "^6.1.1",
    "newman": "^5.2.0",
    "newman-reporter-htmlextra": "^1.19.2",
    "nodemon": "^2.0.7",
    "source-map-support": "^0.5.19",
    "typescript": "~4.1.2",
    "typescript-eslint": "0.0.1-alpha.0"
  }
}
